                                        <elementRef key="settlement"/>
                                        <elementRef key="institution" minOccurs="0"/>
                                        <elementRef key="repository"/>
                                        <elementRef key="collection"/>
                                        <elementRef key="idno"/>
                                    </sequence>
                                    <elementRef key="msName"/>
