                            </content>
                        </elementSpec>
                        <elementSpec ident="origPlace" mode="change">
                            <content>
                                <macroRef key="macro.phraseSeq"/>
                            </content>
                            <attList>
                                <attDef ident="cert" mode="change" usage="rec"/>
