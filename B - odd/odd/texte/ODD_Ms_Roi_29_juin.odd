                                        <valItem ident="chars">
                                            <gloss>caractères</gloss>
                                        </valItem>
                                        <valItem ident="lignes">
                                            <gloss>lignes</gloss>
                                        </valItem>
                                        <valItem ident="columns">
                                            <gloss>colonnes</gloss>
                                        </valItem>
                                        <valItem ident="pages">
                                            <gloss>pages</gloss>
                                        </valItem>
                                        <valItem ident="quires">
                                            <gloss>cahiers</gloss>
                                        </valItem>
                                    </valList>
                                </attDef>
                                <!--<attDef ident="dim" mode="delete">
                                </attDef>-->
                                <!-- Je le garde pour le moment, à réfléchir -->
                                <!--<attDef ident="cert" mode="delete"/>
                                <attDef ident="resp" mode="delete"/>-->
