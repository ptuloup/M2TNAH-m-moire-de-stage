                                    <elementRef key="adminInfo" minOccurs="0"/>
                                    <elementRef key="surrogates"
                                        minOccurs="0"/>
                                    <elementRef key="listBibl" minOccurs="0"/>
                                </sequence>
                            </content>
                        </elementSpec>
                        <elementSpec ident="additions" mode="change">
                            <content>
