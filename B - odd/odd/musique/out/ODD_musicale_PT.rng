   <define name="mei_source">
      <element name="source" ns="http://www.music-encoding.org/ns/mei">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Une description bibliographique d'une source utilisée dans la création du fichier électronique.</a:documentation>
         <zeroOrMore>
            <ref name="mei_model.headLike"/>
         </zeroOrMore>
         <zeroOrMore>
            <choice>
               <ref name="mei_locus"/>
               <ref name="mei_locusGrp"/>
            </choice>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="mei_model.biblLike"/>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="mei-source-check_source_target-constraint-rule-22">
            <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:math="http://exslt.org/math"
                      context="mei:source/@target">
               <sch:assert role="warning" test="not(normalize-space(.) eq '')">L'attribut @target
                  doit avoir du contenu.</sch:assert>
               <sch:assert role="warning"
                           test="every $i in tokenize(., '\s+') satisfies substring($i,2)=//mei:*[local-name() eq 'source' or local-name() eq 'manifestation']/@xml:id or matches($i, '^([a-z]+://|\.{1,2}/)')">Chaque valeur de @target doit correspondre à l'attribut @xml: id d'un élément
                  source ou de manifestation ou être un URI externe.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="mei-source-check_source_target-constraint-rule-23">
            <sch:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:math="http://exslt.org/math"
                      context="mei:source/@target">
               <sch:assert role="warning" test="not(normalize-space(.) eq '')">@target attribute should
            have content.</sch:assert>
               <sch:assert role="warning"
                           test="every $i in tokenize(., '\s+') satisfies substring($i,2)=//mei:*[local-name()              eq 'source' or local-name() eq 'manifestation']/@xml:id or matches($i, '^([a-z]+://|\.{1,2}/)')">Each value in @target should correspond to the @xml:id attribute of a source or
            manifestation element or be an external URI.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="mei_att.common.attributes"/>
         <ref name="mei_att.authorized.attributes"/>
         <ref name="mei_att.bibl.attributes"/>
         <ref name="mei_att.componentType.attributes"/>
         <ref name="mei_att.dataPointing.attributes"/>
         <ref name="mei_att.pointing.attributes"/>
         <ref name="mei_att.recordType.attributes"/>
         <ref name="mei_att.targetEval.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="mei_sourceDesc">
