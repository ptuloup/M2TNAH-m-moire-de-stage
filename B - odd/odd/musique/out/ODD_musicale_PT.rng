   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-att.duration.quality-check_duplex_quality-constraint-rule-24">
      <sch:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:math="http://exslt.org/math"
                context="mei:note[@dur.quality='duplex']">
         <sch:assert test="@dur='longa'">
            Duplex quality can only be used with longas (in Ars antiqua).
          </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-att.duration.quality-check_duplex_quality-constraint-rule-25">
      <sch:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
